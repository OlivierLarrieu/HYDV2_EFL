prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
modules=${exec_prefix}/lib/ethumb/modules
module_arch=v-1.9

Name: ethumb
Description: Thumbnail Generator Library
Version: 1.9.1
Requires.private: edje >= 1.9.1 ecore-imf >= 1.9.1 ecore-file >= 1.9.1 ecore-evas >= 1.9.1 ecore >= 1.9.1 eo >= 1.9.1 evas >= 1.9.1 eina >= 1.9.1  
Libs: -L${libdir} -lethumb
Cflags: -I${includedir}/efl-1 -I${includedir}/ethumb-1
